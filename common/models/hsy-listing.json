{
  "name": "HsyListing",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "content": {
      "type": "text"
    },
    "imageIds": {
      "type": [
        "string"
      ]
    },
    "lastUpdated": {
      "type": "date",
      "required": true
    },
    "location": {
      "type": "geopoint",
      "comments": "location of a listing"
    },
    "ownerId": {
      "type": "string",
      "id": true,
      "required": true,
      "index": true
    },
    "price": {
      "type": "number",
      "required": true
    },
    "title": {
      "type": "string",
      "id": false,
      "required": true,
      "description": ""
    },
    "type": {
      "type": "number",
      "comments": "0 for ROOMMATE_WANTED, 1 for ROOM_WANTED"
    },
    "uid": {
      "type": "string",
      "id": true,
      "required": true,
      "index": true,
      "description": "unique id"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
